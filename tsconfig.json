{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "jsx": "react-jsx",

    "module": "ESNext",
    "moduleResolution": "node",

    "baseUrl": "frontend",

    "paths": {

    },
    "allowJs": true,
    "esModuleInterop": true,

    "strict": true,
    "skipLibCheck": true, 
    "sourceMap": true
  },
  "include": ["**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"]
}